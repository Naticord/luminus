<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="InputHighlightBackground">#C2C2C2</Color>
    <Color x:Key="Background">#EEEEEE</Color>
    <Color x:Key="Background2">#D9D9D9</Color>
    <Color x:Key="Foreground">#6D6D6D</Color>
    <Color x:Key="ForegroundLumina">#000</Color>
    <Color x:Key="InputBackground">#D9D9D9</Color>
    <Color x:Key="InputBackground2">#C4C4C4</Color>


    <SolidColorBrush x:Key="InputHighlightBackgroundBrush" Color="{StaticResource InputHighlightBackground}"/>
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource Background}"/>
    <SolidColorBrush x:Key="Background2Brush" Color="{StaticResource Background2}"/>
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource Foreground}"/>
    <SolidColorBrush x:Key="ForegroundLuminaBrush" Color="{StaticResource ForegroundLumina}"/>
    <SolidColorBrush x:Key="InputBackgroundBrush" Color="{StaticResource InputBackground}"/>

    <SolidColorBrush x:Key="InputBackground2Brush" Color="{StaticResource InputBackground2}"/>
</ResourceDictionary>